{
    "name": "vapor template",
    "excluding": [

    ],
    "display": [
        { 
            "var": "fluent",
            "display": "would you like to use fluent?",
            "choices": ["true", "false"]
        },
        {
            "var": "fluentdb",
            "display": "which fluent database?",
            "choices": [
                "SQLite",
                "PostgreSQL",
                "MySQL"
            ],
            "matchCondition": {
                "var": "fluent",
                "equals": "true"
            }
        },
        {
            "var": "fluentversion",
            "display": "which version?",
            "default": "3.0.0",
            "matchCondition": {
                "var": "fluent",
                "equals": "true"
            }
        }
    ]
}